{
  "name": "@menhera-bot/gateway",
  "version": "1.0.0",
  "engines": {
    "node": ">=18"
  },
  "description": "Menhera's bridge to Discord's Gateway. With this, Menhera can restart the Events package without losing the shards sessions.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "typecheck": " tsc --project tsconfig.json --noEmit",
    "start": "NODE_ENV=PRODUCTION node .",
    "dev": "NODE_ENV=DEVELOPMENT node -r dotenv/config .",
    "dev:nomicroservices": "NODE_ENV=DEVELOPMENT NOMICROSERVICES=true node -r dotenv/config ."
  },
  "author": "Luxanna Crownguard (ySnoopyDogy)",
  "dependencies": {
    "@sentry/node": "^7.24.2",
    "@sentry/tracing": "^7.24.2",
    "nanoid": "v3",
    "net-ipc": "^2.0.1"
  },
  "repository": "https://github.com/MenheraBot/MenheraBot",
  "license": "MIT",
  "readme": "https://github.com/MenheraBot/MenheraBot#readme",
  "devDependencies": {
    "@types/node": "^18.0.6"
  }
}
