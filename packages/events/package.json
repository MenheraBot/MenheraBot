{
  "name": "@menhera-bot/events",
  "type": "module",
  "version": "6.0.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "description": "The core package of Menhera. This package process all interactions that Menhera receives from users",
  "main": "dist/index.js",
  "repository": "https://github.com/MenheraBot/MenheraBot",
  "license": "AGPL-3.0-only",
  "readme": "https://github.com/MenheraBot/MenheraBot#readme",
  "author": "Luan Caliel (ySnoopyDogy)",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --project tsconfig.build.json",
    "postbuild": "mv dist/src uwu && rm -rf dist && mv uwu dist",
    "typecheck": " tsc --project tsconfig.json --noEmit",
    "print-version": "echo version=$npm_package_version",
    "build:dev": "pnpm build && pnpm dev",
    "start": "VERSION=$npm_package_version NODE_ENV=production node .",
    "dev": "VERSION=$npm_package_version NODE_ENV=development node -r dotenv/config .",
    "beta": "VERSION=$npm_package_version NODE_ENV=development node .",
    "dev:nomicroservices": "VERSION=$npm_package_version NODE_ENV=development NOMICROSERVICES=true node -r dotenv/config ."
  },
  "dependencies": {
    "@discordeno/bot": "22.0.1-next.780f212",
    "@sentry/node": "^7.120.4",
    "axios": "^1.12.2",
    "i18next": "^25.6.0",
    "i18next-fs-backend": "^2.6.0",
    "ioredis": "^5.8.1",
    "md5": "^2.3.0",
    "mongoose": "6.13.6",
    "net-ipc": "^2.2.2",
    "pokersolver": "^2.1.4",
    "prom-client": "^15.1.3",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/md5": "^2.3.6",
    "@types/string-similarity": "^4.0.2"
  }
}
