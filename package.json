{
  "name": "menhera-bot",
  "version": "2.9.5-dev",
  "description": "Uma bot brasileira de RPG para o Discord",
  "main": "dist/shard.js",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "husky install",
    "build": "rm -rf dist/* && babel src --extensions \".js,.ts\" --ignore *.spec.js --out-dir dist --copy-files --verbose",
    "start": "cross-env NODE_ENV=production node shard.js",
    "dev": "npm run build && cross-env NODE_ENV=development node .",
    "lint:fix": "eslint --fix .",
    "lint:test": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "docker:build": "docker build -t menhera .",
    "docker:run": "docker run -d --name MenheraBot menhera"
  },
  "keywords": [],
  "author": "Luxanna Crownguard (ySnoopyDogy)",
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "@sentry/node": "^6.8.0",
    "@sentry/tracing": "^6.8.0",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "bufferutil": "^4.0.3",
    "cross-env": "^7.0.3",
    "dblapi.js": "^2.4.0",
    "discord.js": "^12.5.3",
    "dotenv": "^8.6.0",
    "erlpack": "github:discord/erlpack",
    "ffmpeg-static": "^4.3.0",
    "fs-extra": "^9.1.0",
    "i18next": "^19.9.2",
    "i18next-node-fs-backend": "^2.1.3",
    "millify": "^3.5.2",
    "moment": "^2.28.0",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.12.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "string-table": "^0.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ySnoopyDogy/MenheraBot"
  },
  "license": "AGPL-3.0",
  "readme": "https://github.com/ySnoopyDogy/MenheraBot#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/fs-extra": "^9.0.12",
    "@types/i18next-node-fs-backend": "^2.1.1",
    "@types/jest": "^26.0.24",
    "@types/moment-duration-format": "^2.2.3",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
