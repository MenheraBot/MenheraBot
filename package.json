{
  "name": "menhera-bot",
  "version": "2.9.1-dev",
  "description": "Uma bot brasileira de RPG para o Discord",
  "main": "shard.js",
  "scripts": {
    "build": "rm -rf dist/* && babel src --extensions \".js,.ts\" --ignore *.spec.js --out-dir dist --copy-files --source-maps --verbose",
    "start": "cross-env NODE_ENV=production node shard.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "dev:builded": "cross-env NODE_ENV=development node shard.js",
    "lint:fix": "eslint --fix .",
    "lint:test": "eslint .",
    "build:test": "tsc --noEmit",
    "test": "jest",
    "docker:build": "docker build -t menhera .",
    "docker:run": "docker run -d --name MenheraBot menhera"
  },
  "keywords": [],
  "author": "Luxanna Crownguard (ySnoopyDogy)",
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "@sentry/node": "^5.24.2",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "bufferutil": "^4.0.3",
    "dblapi.js": "^2.4.0",
    "discord.js": "^12.5.0",
    "erlpack": "github:discord/erlpack",
    "ffmpeg-static": "^4.3.0",
    "fs-extra": "^9.0.1",
    "i18next": "^19.8.2",
    "i18next-node-fs-backend": "^2.1.3",
    "millify": "^3.5.0",
    "moment": "^2.28.0",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.12.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "string-table": "^0.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ySnoopyDogy/MenheraBot"
  },
  "license": "AGPL-3.0",
  "readme": "https://github.com/ySnoopyDogy/MenheraBot#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@typescript-eslint/parser": "^4.24.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.2",
    "jest": "^26.6.3",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
