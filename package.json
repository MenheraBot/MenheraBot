{
  "name": "menhera-bot",
  "version": "4.1.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "A Brazilian Discord Bot foccused in Fun and RPG",
  "main": "dist/cluster.js",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "husky install",
    "build": "rm -rf dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "VERSION=$npm_package_version NODE_ENV=production node .",
    "dev": "VERSION=$npm_package_version NODE_ENV=development TESTING=true node .",
    "canary": "VERSION=$npm_package_version NODE_ENV=development node .",
    "run:dev": "npm run build && npm run dev",
    "deploy": "pm2 start \"npm run start\" --name MenheraBot ",
    "lint:fix": "eslint --fix .",
    "lint:test": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests --no-cache",
    "test:debug": "jest --passWithNoTests --runInBand --no-cache"
  },
  "keywords": [
    "discord-bot",
    "fun",
    "rpg",
    "brazil"
  ],
  "author": "Luxanna Crownguard (ySnoopyDogy)",
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "axios": "^0.27.2",
    "bufferutil": "^4.0.6",
    "dayjs": "^1.11.4",
    "discord-hybrid-sharding": "^1.5.3",
    "discord.js-light": "^4.7.0",
    "dotenv": "^16.0.0",
    "erlpack": "^0.1.4",
    "i18next": "^21.6.16",
    "i18next-fs-backend": "^1.1.4",
    "ioredis": "^5.0.4",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "mongoose": "^6.2.8",
    "string-similarity": "^4.0.4",
    "tweetnacl": "^1.0.3",
    "utf-8-validate": "^5.0.9",
    "ws": "^8.5.0",
    "zlib-sync": "^0.1.7"
  },
  "repository": "https://github.com/MenheraBot/MenheraBot",
  "license": "AGPL-3.0",
  "readme": "https://github.com/MenheraBot/MenheraBot#readme",
  "devDependencies": {
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^27.4.1",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.3.0",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.0.2",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
