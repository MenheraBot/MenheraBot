{
  "name": "menhera-bot",
  "version": "3.2.0",
  "engines": {
    "node": "^=16.6.0"
  },
  "description": "Uma bot brasileira de RPG para o Discord",
  "main": "dist/shard.js",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "husky install",
    "build": "rm -rf dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "NODE_ENV=production node .",
    "dev": "NODE_ENV=development node .",
    "run:dev": "npm run build && npm run dev",
    "lint:fix": "eslint --fix .",
    "lint:test": "eslint .",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t menhera .",
    "docker:run": "docker run -d --name MenheraBot menhera",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "keywords": [],
  "author": "Luxanna Crownguard (ySnoopyDogy)",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@sentry/node": "^6.13.1",
    "@sentry/tracing": "^6.13.1",
    "axios": "^0.21.4",
    "bufferutil": "^4.0.3",
    "discord.js-light": "timotejroiko/discord.js-light#v4",
    "dotenv": "^10.0.0",
    "i18next": "^21.0.0",
    "i18next-fs-backend": "^1.1.1",
    "ioredis": "^4.27.9",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^6.0.7",
    "ws": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ySnoopyDogy/MenheraBot"
  },
  "license": "AGPL-3.0",
  "readme": "https://github.com/ySnoopyDogy/MenheraBot#readme",
  "devDependencies": {
    "@types/i18next-fs-backend": "^1.1.1",
    "@types/ioredis": "^4.27.4",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.0.3",
    "@types/moment-duration-format": "^2.2.3",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "optionalDependencies": {
    "erlpack": "^0.1.3"
  }
}
