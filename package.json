{
  "name": "menhera-bot",
  "version": "2.9.5-dev",
  "description": "Uma bot brasileira de RPG para o Discord",
  "main": "dist/shard.js",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "husky install",
    "build": "rm -rf dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && cp -R ./src/locales ./dist/locales",
    "start": "NODE_ENV=production node .",
    "dev": "NODE_ENV=development node .",
    "lint:fix": "eslint --fix .",
    "lint:test": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "docker:build": "docker build -t menhera .",
    "docker:run": "docker run -d --name MenheraBot menhera"
  },
  "keywords": [],
  "author": "Luxanna Crownguard (ySnoopyDogy)",
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@sentry/node": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "axios": "^0.21.1",
    "bufferutil": "^4.0.3",
    "dblapi.js": "^2.4.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "erlpack": "^0.1.3",
    "fs-extra": "^10.0.0",
    "i18next": "^20.3.5",
    "i18next-node-fs-backend": "^2.1.3",
    "ioredis": "^4.27.7",
    "millify": "^4.0.0",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.13.5",
    "tsconfig-paths": "^3.10.1",
    "tscpaths": "0.0.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ySnoopyDogy/MenheraBot"
  },
  "license": "AGPL-3.0",
  "readme": "https://github.com/ySnoopyDogy/MenheraBot#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/i18next-node-fs-backend": "^2.1.1",
    "@types/ioredis": "^4.26.6",
    "@types/jest": "^26.0.24",
    "@types/moment-duration-format": "^2.2.3",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
