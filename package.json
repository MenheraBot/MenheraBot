{
  "name": "menhera-bot",
  "version": "5.0.3",
  "description": "A Brazilian Discord Bot foccused in Fun and RPG",
  "private": true,
  "scripts": {
    "rest": "yarn workspace @menhera-bot/rest",
    "gateway": "yarn workspace @menhera-bot/gateway",
    "events": "yarn workspace @menhera-bot/events",
    "lint": "yarn workspace @menhera-bot/eslint-config",
    "test": "TESTING=true node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build:all": "concurrently \"yarn rest build\" \"yarn gateway build\" \"yarn events build\"",
    "run:lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MenheraBot/MenheraBot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MenheraBot/MenheraBot/issues"
  },
  "workspaces": {
    "hoistDependencies": true,
    "packages": [
      "packages/*"
    ]
  },
  "homepage": "https://github.com/MenheraBot/MenheraBot#readme",
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "discordeno": "^17.1.0"
  }
}
